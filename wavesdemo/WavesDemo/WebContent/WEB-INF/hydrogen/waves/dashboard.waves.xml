<waves>
	<config>
		<field name="onerror.email">wavesdev@hydrogen.com</field>
		<field name="transaction.attribute">requirednew</field>
		<field name="onerror.email">wavesdev@hydrogen.com</field>
		<field name="onvalidation.email">wavessupport@hydrogen.com</field>
		<field name="waves.transactionid">generate_inbound/redirect/outbound</field>
		<field name="fault.tolerance_level">auto_start/manual</field>
	</config>
	<wave on="main_page/onload">
		<input path="assets/schema/user_req.properties" type="javax.servlet.http.HttpServletRequest">
			<wave to="dashboard/get_detail">
				<field name="action" type="string">dashboard/get_details</field>
				<field name="userId" path="parameter.principle" type="string" />
			</wave>
			<wave to="dashboard/monitor">
				<field name="action" type="string">main_page/onload</field>
				<field name="userId" path="parameter.principle" type="string" />
			</wave>
		</input>
		<output path="assets/schema/dashboard_detail.json" type="javax.servlet.http.HttpServletResponse">
			<wave from="dashboard/get_detail">
				<field name="dashboard.domains.domainName" path="domains.name" type="string" />
				<field name="dashboard.domains.domainSts" path="domains.status" type="string" />
			</wave>
		</output>
	</wave>
	<wave on="dashboard/get_detail">
		<input path="assets/schema/user_info.properties" type="com.waves.demo.model.UserInfo" />
		<output path="assets/schema/dashboard_info.properties" type="com.waves.demo.model.DashboardInfo" />
	</wave>
	<wave on="dashboard/monitor">
		<input path="assets/schema/user_info.properties" type="com.waves.demo.model.UserInfo" />
	</wave>
</waves>